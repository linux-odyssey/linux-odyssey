<h1 class='heading'>Commands</h1>
<div>
  <h2 class='heading'>Error commands</h2>
  <div class='sort'>
    <a href='/commands?order=asc'><div class='btn'>
        <h3>Ascending</h3>
      </div></a>
    <a href='/commands?order=desc'><div class='btn'>
        <h3>Descending</h3>
      </div></a>
  </div>
  <table class='data-table'>
    <tr>
      <th>Command</th>
      <th>User</th>
      <th>Quest</th>
      <th>Session</th>
      <th>Error</th>
      <th>Created At</th>
    </tr>
    {{#each errorCommands}}
      <tr>
        <td>{{command}}</td>
        <td>{{user}}</td>
        <td>{{quest}}</td>
        <td><a href='/sessions/{{session}}'>Detail</a></td>
        <td>{{error}}</td>
        <td>{{createdAt}}</td>
      </tr>
    {{/each}}
  </table>
  <div class='pagination sort'>
    <a href='/commands?nextKey={{nextKey}}&order={{order}}'><div class='btn'>
        <h3>Next</h3>
      </div></a>
  </div>
</div>