{{#with session}}<h1 class='heading'>Session {{_id}}</h1>
  <div>

    <table class='data-table'>
      <tr>
        <th>Quest</th>
        <td>{{quest}}</td>
      </tr>
      <tr>
        <th>User</th>
        <td>{{user}}</td>
      </tr>
      <tr>
        <th>Status</th>
        <td>{{status}}</td>
      </tr>
      <tr>
        <th>Created At</th>
        <td>{{createdAt}}</td>
      </tr>
      <tr>
        <th>Finished At</th>
        <td>{{finishedAt}}</td>
      </tr>
      <tr>
        <th>Used Time</th>
        <td>{{usedTime}}</td>
      </tr>
      <tr>
        <th>Command Count</th>
        <td>{{commands.length}}</td>
      </tr>
    </table>

    <h2 class='heading'>Commands</h2>

    <table class='data-table'>
      <tr>
        <th>Stage</th>
        <th>PWD</th>
        <th>Input</th>
        <th>Output</th>
        <th>Error</th>
        <th>Input Time</th>
      </tr>
      {{#each commands}}
        <tr>
          <td>{{stage}}</td>
          <td>{{pwd}}</td>
          <td>{{command}}</td>
          <td>{{output}}</td>
          <td>{{error}}</td>
          <td>{{createdAt}}</td>
        </tr>
      {{/each}}
    </table>

  </div>{{/with}}